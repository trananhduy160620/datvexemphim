    <script src="/register/countdown.min.js"></script>
    <script src="/register/odometer.min.js"></script>
    <script src="/register/viewport.jquery.js"></script>
    <script src="/register/nice-select.js"></script>
    <script src="/register/main.js"></script>
  </body>
  <div style="position: absolute; top: 0px"></div>
